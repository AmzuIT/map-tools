{
  "name": "gmplus",
  "version": "0.3.0",
  "description": "Gmplus.js is a Google Maps Feature-rich Javascript wrapper that makes things like: Marker filtering, asynchronous loading, working with TopoJSON or GeoJSON, animation and more. Much simpler with an easy-to-use API.",
  "main": "lib/index.js",
  "scripts": {
    "prestart": "npm install; npm run dist",
    "start": "browser-sync start --server --index examples/index.html",
    "test": "NODE_PATH=lib mocha -r ./test/bootstrap.js --recursive test",
    "coverage": "NODE_PATH=lib istanbul cover --report html node_modules/.bin/_mocha -r ./test/bootstrap.js --recursive test/**/*.Spec.js; browser-sync start --server --startPath coverage/ --index index.html",
    "predist": "NODE_PATH=lib browserify lib/index.js > dist/gmplus.js",
    "dist": "node ./scripts/dist.js",
    "pretest": "jshint lib/index.js",
    "benchmark": "browser-sync start --server --index benchmark/index.html",
    "release": "node ./scripts/release.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yagoferrer/gmplus.git"
  },
  "author": "Yago Ferrer <yago.ferrer@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yagoferrer/gmplus/issues"
  },
  "keywords": [
    "google maps",
    "gmaps",
    "google",
    "topojson",
    "geojson",
    "markers",
    "maps",
    "gmaps",
    "gmplus"
  ],
  "homepage": "https://github.com/yagoferrer/gmplus",
  "devDependencies": {
    "angular": "^1.3.11",
    "benchmark": "^1.0.0",
    "bower": "^1.3.12",
    "browser-sync": "^2.0.1",
    "browserify": "^8.1.1",
    "chai": "^1.10.0",
    "codeclimate-test-reporter": "0.0.4",
    "coveralls": "^2.11.2",
    "gmaps": "^0.4.12",
    "install": "^0.1.8",
    "istanbul": "^0.3.5",
    "jquery": "^2.1.3",
    "jshint": "^2.6.0",
    "jslint": "^0.7.3",
    "lodash": "^3.0.1",
    "mocha": "^2.1.0",
    "npm": "^2.2.0",
    "platform": "^1.3.0",
    "sinon": "^1.12.2",
    "sinon-chai": "^2.6.0",
    "uglify-js": "^2.4.16"
  },
  "dependencies": {
    "crossfilter": "^1.3.11",
    "topojson": "^1.6.18"
  }
}
